generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model pottery {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  password String
}

model products {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  date        String
  description String
  email       String
  /// Nested objects had no data in the sample dataset to introspect a nested type.
  images      Json
  name        String
}

model user {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  bio   String
  city  String
  email String
  name  String
  state String
}
